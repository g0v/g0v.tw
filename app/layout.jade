!!!5
html(ng-app="g0v.tw",ng-cloak)
    head
        meta(charset="utf-8")
        meta(name="viewport",content="width=device-width, initial-scale=1.0")
        link(rel="SHORTCUT ICON",href="/g0v.ico")
        title
            block title
        link(rel="stylesheet", type="text/css", href="/css/app.css")
        script(src='//cdnjs.cloudflare.com/ajax/libs/jquery/1.8.2/jquery.min.js')
        script(src='//ajax.googleapis.com/ajax/libs/angularjs/1.1.5/angular.min.js')
        script(src='//cdn.firebase.com/v0/firebase.js')
        script(src='/js/vendor.js')
        script(src='/js/app.js')
        script(type="text/javascript").
          (function(d, s, id) {
              var js, fjs = d.getElementsByTagName(s)[0];
              if (d.getElementById(id)) return;
              js = d.createElement(s); js.id = id;
              js.src = "//connect.facebook.net/zh_TW/all.js#xfbml=1";
              fjs.parentNode.insertBefore(js, fjs);
          }(document, 'script', 'facebook-jssdk'));

        script(type="text/javascript").
          window.___gcfg = {lang: 'zh-TW'};

          (function() {
              var po = document.createElement('script'); po.type = 'text/javascript'; po.async = true;
              po.src = 'https://apis.google.com/js/plusone.js';
              var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(po, s);
          })();

    body
        #header
            #header_inner_container
                #logo
                    a(href="/index.html"): img(src="/imgs/g0v-240-invert.png")

                #social_block.hidden-phone
                    .fb-like(data-href="http://facebook.com/g0v.tw",data-width="500",data-colorscheme="dark",data-layout="box_count",data-show-faces="true",data-send="true")
                    .g-plusone(data-size="tall",data-href="http://g0v.tw")

                div.clearfix

        #nav_bar
            #nav_main
                #nav_index.nav_tab
                    a(href="/")
                        span 首頁
                #nav_about.nav_tab
                    a(href="/about.html")
                        span 關於我們
                #nav_archives.nav_tab
                    a(href="http://hack.g0v.tw/project", target="_blank")
                        span 所有成果
                #nav_join.nav_tab
                    a(href="/join.html")
                        span 加入我們
                #nav_news.nav_tab
                    a(href="/news.html")
                        span 最新消息
                div.clearfix

        #container

            block content

    #footer.hidden-phone
        #sitemap-panel
            .map-entry.hackathon
                .map-caption.map-caption-hot: 國民大會黑客松
                .map-item: a(href="http://registrano.com/events/g0v-hackath4n-taipei") 台北場報名網址
                .map-item: a(href="http://hack.g0v.tw/g0v-hackath4n") 黑客松共筆
                #talk
                 div
                   | 8/10 黑客松
                   br
                   | 報名網址
            .map-entry
                .map-caption: 零時政府
                //- .map-item: a(href="/about.html") 關於我們
                //- .map-item: a(href="/sitemap.html") 網站地圖
                .map-item: a(href="http://www.facebook.com/g0v.tw") 臉書專頁
                .map-item: a(href="https://plus.google.com/102893633954450772289",rel="publisher") G+ 專頁
                .map-item: a(href="http://www.facebook.com/groups/387816094628136/") 後勤中心
                .map-item: a(href="http://data.g0v.tw/") 資料中心
                .map-item: a(href="http://g0v.tw/transaction.html") 收支明細
            .map-entry
                .map-caption: 專案
                .map-item: a(href="http://github.com/g0v") 源碼集散地
                .map-item: a(href="http://hack.g0v.tw/") 開發者首頁
                .map-item: a(href="http://github.com/g0v/g0v.tw/wiki/Add-project") 增加專案
            .map-entry
                .map-caption: 資訊
                .map-item: a(href="http://blog.g0v.tw") 部落格
                .map-item: a(href="https://twitter.com/search?q=%23g0v&src=hash") 推特
            .map-entry
                .map-caption: 活動
                .map-item: a(href="http://www.flickr.com/photos/tkirby/collections/72157632969946032/") 活動照片
                .map-item: a(href="http://www.youtube.com/playlist?list=PLOmVOzQIKKkTrOqF-BdKqK0iLM6MspSGM") 活動實況
            .map-entry
                .map-caption: 指南
                .map-item: a(href="https://github.com/g0v/dev/wiki/g0v-newstuff") 新手指南
                .map-item: a(href="http://g0v.tw/join.html") 參與我們


        #social_plugins

            div.footer-brand
                | g0v.tw 2013 |
                img(src="/imgs/g0v-icon-invert.png")
                .build-id(ng-controller="BuildIdCtrl")
                  a(href="https://github.com/g0v/g0v.tw") {{buildId}}


